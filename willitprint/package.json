{
  "name": "willitprint",
  "version": "1.0.0",
  "description": "Your degenerate options whisperer",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd willitprint/frontend && npm run dev",
    "dev:backend": "cd willitprint/backend && npm run dev",
    "start:frontend": "cd willitprint/frontend && npm run preview",
    "start:backend": "cd willitprint/backend && npm run start",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd willitprint/frontend && npm run build",
    "build:backend": "cd willitprint/backend && npm run build",
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd willitprint/frontend && npm install",
    "install:backend": "cd willitprint/backend && npm install"
  },
  "keywords": [
    "options",
    "trading",
    "wallstreetbets",
    "react",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 